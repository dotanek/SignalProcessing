<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SignalProcessing.View.Styles">
    <!-- Colors -->
    <Color x:Key="ColorPrimary1">#2E356E</Color>
    <Color x:Key="ColorPrimary2">#1B1F46</Color>
    
    <!-- Brushes -->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}" />
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2}" />

</ResourceDictionary>